#include <limits>
#include <iostream>
#include <string>
using namespace std;
int main(){

	int n=0;
	cout<<"enter the number:";
	cin>>n;
	char c='67';
	if(c<='93')
	{
		cout<<"Enter the number";
	}else{
		if(n%2==0){
		cout<<"NUMBER IS EVEN";


		}else{
		cout<<"NUMBER IS ODD";
		}
	}
	return 0;


}

