#include <limits>
#include <iostream>
#include <string>
using namespace std;
float main(){
	float n;
	cout<<"enter the number:";
	cin>>n;

		if(n%2==0){
		cout<<"NUMBER IS EVEN";


		}else{
		cout<<"NUMBER IS ODD";

}
	return 0;


}

