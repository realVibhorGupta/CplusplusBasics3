#include<iostream>
using namespace std;
 int i;
double j=22;
float k;
char a;
long l;
int main()
{
	cout<<"global variable is:"<<i<<endl<<"hello"<<endl;
	cout<<"global variable is:"<<j<<endl;
	cout<<"global variable is:"<<k<<endl;
	cout<<"global variable is:"<<l<<endl;
	cout<<"global variable is:"<<a;
}




